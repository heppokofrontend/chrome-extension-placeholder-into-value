{
  "manifest_version": 3,
  "background": {
    "service_worker": "/worker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/content_scripts.js"]
    }
  ],
  "icons": {
    "128": "icon.png",
    "16": "icon.png"
  },
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "permissions": ["contextMenus"],
  "host_permissions": ["<all_urls>"],
  "version": "1.0.2",
  "default_locale": "en"
}
