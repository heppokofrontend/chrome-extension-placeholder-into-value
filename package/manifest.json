{
  "background": {
    "service_worker": "/worker.js"
  },
  "content_scripts": [
    {
      "js": ["/content_scripts.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "128": "icon.png",
    "16": "icon.png"
  },
  "manifest_version": 3,
  "name": "Image Controler",
  "description": "画像を操作できるツール",
  "permissions": ["scripting", "contextMenus", "notifications"],
  "host_permissions": ["<all_urls>"],
  "version": "1.0.0"
}
